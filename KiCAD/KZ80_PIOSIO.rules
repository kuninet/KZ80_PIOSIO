
(rules PCB KZ80_PIOSIO
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 6051)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.3)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 1.8)
    )
  )
  (rule
    (width 250.0)
    (clear 200.2)
    (clear 125.0 (type smd_to_turn_gap))
    (clear 50.0 (type smd_smd))
  )
  (padstack "Via[0-1]_800:400_um"
    (shape
      (circle F.Cu 800.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 800.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_800:400_um" "Via[0-1]_800:400_um" default
  )
  (via 
    "Via[0-1]_800:400_um-kicad_default" "Via[0-1]_800:400_um" "kicad_default"
  )
  (via 
    "Via[0-1]_800:400_um-BOLD" "Via[0-1]_800:400_um" BOLD
  )
  (via_rule
    default "Via[0-1]_800:400_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_800:400_um-kicad_default"
  )
  (via_rule
    BOLD "Via[0-1]_800:400_um-BOLD"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    "Net-(C7-Pad1)" "Net-(C8-Pad1)" "Net-(JP1-Pad1)" "Net-(C6-Pad1)" A7 /D2 BRDY /D7
    "Net-(U2-Pad22)" /D6 "~INT-J" "PIO-CE" "Net-(U1-Pad7)" A0 CLK A1
    PA7 PB0 PA6 PB1 PA5 PB2 PA4 PB3
    PB4 PA3 PB5 PA2 PB6 PA1 PB7 PA0
    ~RD ~ASTB ~IORQ ~BSTB ~M1 ARDY /D5 /D0
    /D4 /D1 /D3 "Net-(U1-Pad16)" CTSA RTSA "SIO-CE" TxDA
    "SIO-CLK" RxDA "Net-(U1-Pad11)" "Net-(U1-Pad30)" "Net-(U1-Pad10)" RxDB TxDB "Net-(U1-Pad22)"
    RTSB CTSB ~RESET "CTC-CE" SPK "Net-(U4-Pad22)" "Net-(U4-Pad21)" "Net-(U5-Pad1)"
    "Net-(U5-Pad9)" "Net-(U5-Pad2)" "Net-(U5-Pad3)" "Net-(U5-Pad4)" "Net-(U5-Pad5)" "Net-(U5-Pad13)" "Net-(U5-Pad6)" "Net-(U5-Pad14)"
    "Net-(U5-Pad15)" "Net-(JP1-Pad2)" "Net-(U3-Pad7)" A5 A6 "Net-(J7-Pad1)" "Net-(J7-Pad3)" A4
    "Net-(J7-Pad5)" A3 "Net-(J7-Pad7)" "Net-(J7-Pad2)" /~IOR /~IOW /RES /INT
    /A2 /A8 /A10 /A12 /A14 /A15 /A13 /A11
    /A9 /HOLD /HLDA /~MEMW /~MEMR /~WR /~MEMRQ /~BUSREQ
    /~BUSACK /~NMI /~RFSH /~WAIT /HALT "Net-(J8-Pad14)" "Net-(J4-Pad12)" "Net-(J4-Pad11)"
    "Net-(J3-Pad11)" "Net-(J3-Pad12)" "Net-(J2-Pad3)" "Net-(J1-Pad3)" "Net-(J5-Pad5)" "Net-(J5-Pad6)" "Net-(J5-Pad7)"
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class BOLD
    VCC GND
    (clearance_class BOLD)
    (via_rule BOLD)
    (rule
      (width 600.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
)